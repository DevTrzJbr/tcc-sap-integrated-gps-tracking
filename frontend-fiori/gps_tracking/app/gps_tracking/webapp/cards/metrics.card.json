{
  "_version": "1.16.0",
  "sap.app": {
    "type": "card",
    "id": "com.tcc.gpstracking.cards.metrics"
  },
  "sap.card": {
    "type": "Object",
    "configuration": {
      "parameters": {
        "distanceKm": { "value": "-" },
        "totalMinutes": { "value": "-" },
        "averageSpeedKmh": { "value": "-" },
        "movingMinutes": { "value": "-" },
        "stoppedMinutes": { "value": "-" }
      }
    },
    "header": {
      "title": "Métricas da Rota"
    },
    "content": {
      "groups": [
        {
          "title": "Resumo",
          "items": [
            {
              "label": "Distância (km)",
              "value": "{{parameters.distanceKm}}"
            },
            {
              "label": "Tempo total (min)",
              "value": "{{parameters.totalMinutes}}"
            },
            {
              "label": "Velocidade média (km/h)",
              "value": "{{parameters.averageSpeedKmh}}"
            },
            {
              "label": "Tempo em movimento (min)",
              "value": "{{parameters.movingMinutes}}"
            },
            {
              "label": "Tempo parado (min)",
              "value": "{{parameters.stoppedMinutes}}"
            }
          ]
        }
      ]
    }
  }
}
